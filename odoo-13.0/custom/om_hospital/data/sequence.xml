<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for hospital.patient -->
        <record id="hospital_patient_sequence" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hospital appoinment -->
        <record id="hospital_appoinment_sequence" model="ir.sequence">
            <field name="name">Appoinment Sequence</field>
            <field name="code">hospital.appoinment.sequence</field>
            <field name="prefix">AP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hospital Doctor -->
        <record id="hospital_doctor_sequence" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor.sequence</field>
            <field name="prefix">DOC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>