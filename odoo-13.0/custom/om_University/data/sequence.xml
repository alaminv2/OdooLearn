<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!--    Sequence for students-->
        <record id="students_sequence" model="ir.sequence">
            <field name="name">Students Sequence</field>
            <field name="code">studnts.sequence</field>
            <field name="prefix">ST_NO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--    Sequence for Teachers-->
        <record id="teachers_sequence" model="ir.sequence">
            <field name="name">Teachers Sequence</field>
            <field name="code">teachers.sequence</field>
            <field name="prefix">TCHR_NO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
